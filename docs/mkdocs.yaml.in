site_name: ILLIXR Hand Tracking Docs
theme: readthedocs
site_dir: @MKDOCS_BUILD_DIR@
site_url: https://illixr.github.io/hand_tracking
markdown_extensions:
    - codehilite
    - fenced_code
    - footnotes
    - admonition
extra_css:
    - css/code-box-copy.css
    - css/prism.min.css
    - css/styles.css
extra_javascript:
    - js/clipboard.min.js
    - js/code-box-copy.js
    - js/prism.min.js
docs_dir: @MKDOCS_DOCS_DIR@
nav:
    - 'Latest Documentation':
        - 'Intro': index.md
        - 'API': api/html/annotated.html
        - 'License': LICENSE.md
